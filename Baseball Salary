   ],
   "source": [
    "ols = LinearRegression()\n",
    "rfecv = RFECV(estimator=ols, step=1, scoring='neg_mean_squared_error')\n",
    "rfecv.fit(X_train, y_train)\n",
    "rfecv.transform(X_train)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 63,
